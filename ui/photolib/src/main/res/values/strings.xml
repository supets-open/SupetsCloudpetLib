<resources>
    <!--背景色-->
    <color name="m_photo_bg_page">#fff5f7fb</color>
    <dimen name="m_photo_title_bar_height">48dp</dimen>
    <!--顶部导航背景色和标题色-->
    <color name="m_photo_new_title_bar_bg">@color/white</color>
    <!--常见颜色-->
    <color name="white">#ffffff</color>

    <style name="m_photo_line_horizontal">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/m_photo_line_1px</item>
        <item name="android:background">@color/m_photo_line_color</item>
    </style>

    <dimen name="m_photo_line_1px">0.5dp</dimen>

    <color name="m_photo_transparent">#0000</color>

    <!--线颜色-->
    <color name="m_photo_line_color">#e7e7e7</color>
    <!--定义APP主题色-->
    <color name="m_photo_app_color">#ff6778</color>
    <!--占位图-->
    <color name="m_photo_place_holder">#f0f0f0</color>
    <drawable name="m_photo_place_holder">@color/m_photo_place_holder</drawable>

    <string name="m_photo_xuanzhe">选择</string>

    <style name="m_photo_FullscreenTheme" parent="android:Theme.NoTitleBar">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@null</item>
    </style>
</resources>
