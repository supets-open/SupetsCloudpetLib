<resources>
    <style name="PullToRefreshLoading" parent="@android:style/Widget.ProgressBar.Small">
        <item name="android:minWidth">60dip</item>
        <item name="android:maxWidth">60dip</item>
        <item name="android:minHeight">60dip</item>
        <item name="android:maxHeight">60dip</item>
        <item name="android:indeterminateDrawable">@drawable/anim_drawable_pull_to_refresh_loading</item>
    </style>
    <style name="line_horizontal">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/line_1px</item>
        <item name="android:background">@color/line_color</item>
    </style>

    <style name="line_item_horizontal">
        <item name="android:background">@color/bg_page</item>
        <item name="android:layout_height">10dp</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="line_vertical">
        <item name="android:layout_width">@dimen/line_1px</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/line_color</item>
    </style>

    <string name="supet_cube_ptr_pull_down">下拉刷新</string>
    <string name="supet_cube_ptr_pull_down_to_refresh">下拉刷新</string>
    <string name="supet_cube_ptr_refresh_complete">更新完成.</string>
    <string name="supet_cube_ptr_refreshing">加载中...</string>
    <string name="supet_cube_ptr_release_to_refresh">释放刷新</string>

    <string name="page_view_emtpy">暂无数据</string>

</resources>
